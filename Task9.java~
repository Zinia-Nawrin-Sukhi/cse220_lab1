import java.util.Scanner;
public class Task9{
  public static void main(String[]args){
    Scanner sc =new Scanner(System.in);
   int x= sc.nextInt();
   int a[]= new int [x];
   for(int i=0;i<a.length;i++){
     a[i]=sc.nextInt();
     }
    for(int i=0;i<a.length;i++){
     int square= a[i]*a[i];
     }
    public void display (int a[]){
      for(int i=0;i<a.length;i++){
        System.out.println(a[i]);                  
      }
    }
      
      
      
      
      
